{
  "private": true,
  "scripts": {
    "demo": "turbo build --filter=@zap/* && turbo dev --filter=@demo/web --filter=@deprecated/oracle",
    "build": "turbo build --filter=@zap/*",
    "docs": "turbo dev --filter=docs",
    "test": "turbo test",
    "lint": "turbo lint --filter=@zap/* --filter=@demo/web --filter=@deprecated/oracle --no-daemon",
    "clean": "turbo clean",
    "format": "prettier --write --ignore-unknown **/*"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^7.32.0",
    "eslint-plugin-o1js": "^0.4.0",
    "turbo": "^1.12.4",
    "typescript": "^4.9.4"
  },
  "packageManager": "pnpm@9.0.6"
}
